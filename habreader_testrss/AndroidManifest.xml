<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.habreader_testrss"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.habreader_testrss.FeedListActivity"
            android:label="@string/title_feed_list"
            android:parentActivityName="com.example.habreader_testrss.HabreaderActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.habreader_testrss.HabreaderActivity" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.habreader_testrss.HabreaderActivity"
            android:label="@string/title_activity_habreader" >
            <!--intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter-->
        </activity>
        <activity android:name="com.example.habreader_testrss.settings.SettingsActivity" >
        </activity>
        <activity
            android:name="com.example.habreader_testrss.FeedDetailsActivity"
            android:label="@string/title_activity_feed_details" >
        </activity>        
        <activity
            android:name="com.example.habreader_testrss.FeedDetailActivity"
            android:label="@string/title_feed_detail"
            android:parentActivityName=".FeedListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FeedListActivity" />
        </activity>
    </application>

</manifest>
